    "simple-git-hooks": "2.9.0",
    "socket.io-client": "^4.7.2",
    "swagger-ts-generator": "^2.0.6",
    "typescript": "5.2.2",
    "vfonts": "^0.0.3",
    "vite": "4.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-doubleshot": "0.0.12",
    "vue-tsc": "1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
