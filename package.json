    "@lightwing/eslint-config": "^0.0.24",
    "@types/echarts": "^4.9.19",
    "@types/file-saver": "^2.0.5",
    "@types/lodash": "^4.14.199",
    "@types/node": "^20.8.5",
    "@vicons/fa": "^0.12.0",
    "@vitejs/plugin-vue": "4.4.0",
    "@vue/compiler-sfc": "3.3.4",
    "electron": "27.0.0",
    "electron-builder": "24.6.4",
    "eslint": "8.51.0",
    "lint-staged": "14.0.1",
    "naive-ui": "^2.35.0",
