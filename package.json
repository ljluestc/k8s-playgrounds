{
  "name": "k8s-playground",
  "version": "0.0.3",
  "packageManager": "pnpm@8.6.11",
  "description": "k8s playground with NaiveUI+Vue+NestJS+Electron,made by TypeScript",
  "author": "weibaohui <weibaohui@yeah.net>",
  "main": "dist/backend/index.js",
  "scripts": {
    "dev": "rimraf dist && vite",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:backend": "vitest run --config vitest.backend.config.ts",
    "test:backend:watch": "vitest --config vitest.backend.config.ts",
    "test:backend:ui": "vitest --ui --config vitest.backend.config.ts",
    "test:backend:coverage": "vitest run --coverage --config vitest.backend.config.ts",
    "test:frontend": "vitest run --config vitest.frontend.config.ts",
    "test:frontend:watch": "vitest --config vitest.frontend.config.ts",
    "test:frontend:ui": "vitest --ui --config vitest.frontend.config.ts",
    "test:frontend:coverage": "vitest run --coverage --config vitest.frontend.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:integration": "vitest run \"test/integration/**/*.test.ts\"",
    "test:performance": "vitest run \"test/performance/**/*.test.ts\"",
    "test:e2e:watch": "vitest --config vitest.e2e.config.ts",
    "test:e2e:ui": "vitest --ui --config vitest.e2e.config.ts",
    "test:e2e:coverage": "vitest run --coverage --config vitest.e2e.config.ts",
    "test:all": "pnpm test:backend && pnpm test:frontend && pnpm test:integration && pnpm test:performance && pnpm test:e2e && pnpm test:coverage",
    "test:generate": "ts-node scripts/test-generator.ts",
    "test:orchestrate": "ts-node scripts/test-orchestrator.ts",
    "test:ci": "pnpm test:all --reporter=verbose --reporter=json --outputFile=test-results.json",
    "postinstall": "electron-builder install-app-deps",
    "gen": "openapi --input http://127.0.0.1:3007/api-json --output ./src/frontend/generated --useOptions --name=Play",
    "playwright:install": "npx playwright install --with-deps",
    "test:playwright": "playwright test",
    "test:playwright:report": "playwright show-report"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.5",
    "@kubernetes/client-node": "^0.19.0",
    "@nestjs/common": "^10.2.8",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.8",
    "@nestjs/microservices": "^10.2.8",
    "@nestjs/platform-express": "^10.2.8",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.15",
    "@nestjs/websockets": "^10.2.8",
    "axios": "^1.6.1",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "is-base64": "^1.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "^0.44.0",
    "naive-ui": "^2.35.0",
    "node-pty": "^1.0.0",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^0.0.24",
    "@nestjs/testing": "^10.2.8",
    "@playwright/test": "^1.48.0",
    "@testing-library/user-event": "^14.5.1",
    "@testing-library/vue": "^8.0.1",
    "@types/echarts": "^4.9.21",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@vicons/fa": "^0.12.0",
    "@vitejs/plugin-vue": "4.4.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.0.4",
    "@vue/compiler-sfc": "3.3.4",
    "@vue/test-utils": "^2.4.3",
    "electron": "27.0.0",
    "electron-builder": "24.6.4",
    "eslint": "8.51.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "happy-dom": "^12.10.3",
    "lint-staged": "14.0.1",
    "naive-ui": "^2.35.0",
    "rimraf": "5.0.5",
    "simple-git-hooks": "2.9.0",
    "socket.io-client": "^4.7.2",
    "supertest": "^6.3.3",
    "swagger-ts-generator": "^2.0.6",
    "typescript": "5.2.2",
    "vfonts": "^0.0.3",
    "vite": "4.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-doubleshot": "0.0.12",
    "vitest": "^1.0.4",
    "vue-tsc": "1.8.19"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged && pnpm -s test:playwright || true"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "eslint --fix",
      "vitest related --run"
    ],
    "tests/playwright/**/*.ts": [
      "node -e \"console.log('Playwright specs staged, will run in hook')\""
    ],
    "*.{md,json,yml}": [
      "eslint --fix"
    ]
  }
}
