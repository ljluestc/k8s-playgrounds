    "@nestjs/microservices": "^10.2.7",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/schedule": "^3.0.4",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/websockets": "^10.2.7",
    "axios": "^1.5.1",
    "echarts": "^5.4.3",
    "file-saver": "^2.0.5",
    "is-base64": "^1.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
